{
    "containerDefinitions": [
        {
            "name": "weatherapp",
            "image": "387352317739.dkr.ecr.us-east-1.amazonaws.com/django-weatherapp",
            "cpu": 512,
            "memory": 2048,
            "memoryReservation": 1024,
            "portMappings": [
                {
                    "name": "weatherapp-8080-tcp",
                    "containerPort": 8080,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "systemControls": []
        },
        {
            "name": "weatherapp-proxy",
            "image": "387352317739.dkr.ecr.us-east-1.amazonaws.com/django-weatherapp-proxy",
            "cpu": 256,
            "memory": 1024,
            "memoryReservation": 512,
            "portMappings": [
                {
                    "name": "weatherapp-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        }
    ],
    "family": "weatherapp-task-definition",
    "executionRoleArn": "arn:aws:iam::387352317739:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "compatibilities": [
        "EC2",
        "FARGATE"
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "2048",
    "ephemeralStorage": {
        "sizeInGiB": 21
    },
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}